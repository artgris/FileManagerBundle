{
  "name": "artgris/filemanager-bundle",
  "type": "symfony-bundle",
  "description": "FileManager is a simple Multilingual File Manager Bundle for Symfony",
  "keywords": [
    "symfony",
    "bundle",
    "file manager"
  ],
  "homepage": "https://github.com/artgris/FileManagerBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Gribet",
      "email": "a.gribet@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "sensio/framework-extra-bundle": "^6.1",
    "symfony/translation": "^6.0",
    "symfony/twig-bridge": "^6.0",
    "symfony/twig-bundle": "^6.0",
    "symfony/form": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/asset": "^6.0",
    "symfony/mime": "^6.0",
    "symfony/expression-language": "^6.0",
    "oomphinc/composer-installers-extender": "^2.0",
    "symfony/flex": "^2"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.0",
    "symfony/security-bundle": "^6.0",
    "symfony/css-selector": "^6.0",
    "symfony/browser-kit": "^6.0",
    "symfony/var-dumper": "^6.0",
    "symfony/yaml": "^6.0",
    "symfony/templating": "^6.0"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": {
      "symfony/doctrine-bridge": "source",
      "*": "dist"
    },
    "allow-plugins": {
      "composer/installers": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "composer/package-versions-deprecated": true
    }
  },
  "autoload": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\Tests\\": "/Tests/"
    }
  }
}
